<%- include('./partials/home_head.ejs') %>
    <%- include('./partials/nav.ejs') %>
        <section id="hero-section" class="slide background-withcolor"
            style="background: linear-gradient(355deg, #241F1F 90%, #0d0b0b 0%);">
            <div id='stars'></div>
            <div id='stars1'></div>
            <div id='stars2'></div>
            <% sections.forEach(function(section) { %>

                <!-- #1 -->
                <<%= section.tag %>
                    id="<%- section.id %>"
                        class="<%- section.classes %>"
                            >
                            <!-- #2 -->
                            <<%- section.content.tag %>
                                id="<%- section.content.id %>"
                                    class="<%- section.content.classes %>"
                                        >
                                        <!-- #3 -->
                                        <<%- section.content.content.tag %>
                                            id="<%- section.content.content.id %>"
                                                class="<%- section.content.content.classes %>"
                                                    >
                                                    <!-- #4 -->

                                                    <<%- section.content.content.content.tag %>
                                                        id="<%- section.content.content.content.id %>"
                                                            class="<%- section.content.content.content.classes %>"
                                                                attrs="<%- section.content.content.content.attrs %>">
                                                                    <!-- #5 -->
                                                                    <% section.content.content.content.content.forEach(function(comp){
                                                                        %>

                                                                        <<%- comp.tag %>
                                                                            id="<%- comp.id %>"
                                                                                class="<%- comp.classes %>"
                                                                                    style="<%- comp.style %>"
                                                                                        href="<%- comp.href %>"
                                                                                            >

                                                                                            <% if(typeof(comp.content)=='string'
                                                                                                ) { %>
                                                                                                <%= comp.content %>
                                                                                                    <% } %>

                                                                                                        <% if(typeof(comp.content)=='object'
                                                                                                            ) { %>

                                                                                                            <<%-comp.content.tag%>
                                                                                                                >
                                                                                                                <<%-comp.content.content.tag%>
                                                                                                                    >
                                                                                                                    <%= comp.content.content.content
                                                                                                                        %>
                                                                                                                </<%-comp.content.content.tag%>
                                                                                                                >
                                                                                                            </<%-comp.content.tag%>
                                                                                                            >

                                                                                                            <% } %>

                                                                        </<%- comp.tag %>>

                                                                        <% }) %>

                                                    </<%- section.content.content.content.tag %>>
                                        </<%- section.content.content.tag %>>
                            </<%- section.content.tag %>>
                </<%= section.tag %>>
                <% }) %>






        </section>

